{
    "name": "yanselmask/reservable",
    "description": "a package to do reservation",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "yanselmask",
            "email": "info@yanselmask.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yanselmask\\Reservable\\LaraReserveServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Yanselmask\\Reservable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yanselmask\\Reservable\\Tests\\": "tests/"
        }
    }
}
